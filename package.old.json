{
  "name": "@bitsoul/derivative-nft-protocol",
  "version": "1.0.0",
  "description": "Derivative and decentralized NFT protocol",
  "main": "index.js",
  "scripts": {
    "test": "npm run compile && TRACK_GAS=true hardhat test",
    "quick-test": "hardhat test",
    "spdx": "hardhat prepend-spdx-license",
    "size": "npm run compile && hardhat size-contracts",
    "full-deploy-local": "hardhat full-deploy --network local",
    "full-deploy-mumbai": "hardhat full-deploy --network mumbai",
    "full-deploy-verify-mumbai": "hardhat testnet-full-deploy-verify --network mumbai",
    "setup-mumbai": "hardhat setup --network mumbai",
    "coverage": "npm run compile && hardhat coverage --temp temp-artifacts --testfiles test/emptyrun.coverage.ts && hardhat coverage --temp temp-artifacts --testfiles '!test/emptyrun.coverage.ts'",
    "run-env": "npm i && tail -f /dev/null",
    "hardhat": "hardhat",
    "hardhat:kovan": "hardhat --network kovan",
    "hardhat:tenderly-main": "hardhat --network tenderlyMain",
    "hardhat:ropsten": "hardhat --network ropsten",
    "hardhat:main": "hardhat --network main",
    "hardhat:docker": "hardhat --network hardhatevm_docker",
    "hardhat:mumbai": "hardhat --network mumbai",
    "hardhat:matic": "hardhat --network matic",
    "compile": "SKIP_LOAD=true hardhat clean && SKIP_LOAD=true hardhat compile",
    "console:fork": "MAINNET_FORK=true hardhat console",
    "format": "prettier --write .",
    "graph-local-node-start": "cd ./docker && docker-compose up -d",
    "graph-local-node-stop": "cd ./docker &&  docker-compose down -v && docker-compose rm -v && sudo rm -rf data/ipfs data/postgres",
    "node-logs": "cd ./docker &&  docker-compose logs -f -t --tail=\"100\" hardhat-node",
    "graph-node-logs": "cd ./docker &&  docker-compose logs -f -t --tail=\"100\" graph-node",
    "graph-local-codegen": "cd ./subgraph && npm run codegen",
    "graph-local-build": "cd ./subgraph && npm run build",
    "create-local-subgraph-node": "cd ./subgraph &&  npm run create-local",
    "deploy-local-subgraph-node": "cd ./subgraph && npm run deploy-local",
    "remove-local-subgraph-node": "cd ./subgraph && npm run remove-local"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MeitaVerse/derivative-nft-protocol"
  },
  "bugs": {
    "url": "https://github.com/MeitaVerse/derivative-nft-protocol/issues"
  },
  "homepage": "https://github.com/MeitaVerse/derivative-nft-protocol#readme",
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.37.1",
    "@graphprotocol/graph-ts": "^0.29.1",
    "@graphprotocol/hardhat-graph": "https://github.com/graphprotocol/hardhat-graph",
    "@manifoldxyz/royalty-registry-solidity": "^2.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.4",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.3",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@solvprotocol/erc-3525": "git+https://github.com/solv-finance/erc-3525.git",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.4",
    "@types/mocha": "^10.0.1",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "hardhat": "^2.10.2",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-log-remover": "^2.0.2",
    "hardhat-spdx-license-identifier": "^2.1.0",
    "hardhat-tracer": "^1.1.0-rc.8",
    "solc": "^0.8.13",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.0",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@openzeppelin/contracts-upgradeable": "^4.8.0",
    "@solvprotocol/erc-3525": "git+https://github.com/solv-finance/erc-3525.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'contracts/**/*.sol' --pattern 'helpers/**/*.ts'  --pattern 'test/**/*.ts' --pattern 'tasks/**/*.ts'"
    }
  },
  "author": "Bitsoul",
  "contributors": [
    "fengduan",
    "Terry",
    "Sky Lee"
  ],
  "license": "MIT",
  "keywords": [
    "erc3525",
    "erc721",
    "protocol",
    "metaverse",
    "derivative",
    "social",
    "graph",
    "solidity",
    "evm",
    "contracts",
    "core"
  ]
}
